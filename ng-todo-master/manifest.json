{
  "manifest_version": 2,
  "name"            : "ng-Todo",
  "description"     : "Minimal todo app for minimalist",
  "short_name"      : "ng-Todo",
  "version"         : "0.1",
  "browser_action": {
    "default_popup": "index.html",
    "default_icon" : "images/logo.png"
  },
  "permissions": [
    "https://web-cat.ccs.neu.edu/*",
    "debugger",
    "storage"
  ],
  
  "options_page": "options.html"
}
